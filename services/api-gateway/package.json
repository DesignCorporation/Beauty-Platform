{
  "name": "beauty-platform-api-gateway",
  "version": "1.0.0",
  "description": "Beauty Platform API Gateway - Single Entry Point for all microservices",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run generate:services && tsc",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "generate:services": "node scripts/generate-services.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "axios": "^1.7.9",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "keywords": [
    "api-gateway",
    "microservices",
    "beauty-platform",
    "express",
    "proxy"
  ],
  "author": "Beauty Platform Team",
  "license": "MIT"
}